<?xml version="1.0" encoding="utf-8"?>

<set xmlns:android="http://schemas.android.com/apk/res/android"
    android:interpolator="@android:anim/accelerate_decelerate_interpolator">
    <alpha
        android:duration="250"
        android:fromAlpha="0"
        android:toAlpha="1" />
    <translate
        android:duration="250"
        android:fromYDelta="25%"
        android:toYDelta="0%" />
</set>